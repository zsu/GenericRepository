<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <VersionPrefix>1.0.3</VersionPrefix>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>GenericRepository.Mvc</AssemblyName>
    <PackageId>GenericRepository.EntityFrameworkCore.Mvc</PackageId>
    <!--<RuntimeFrameworkVersion>2.1.2</RuntimeFrameworkVersion>-->
    <PackageTargetFallback>$(PackageTargetFallback);dnxcore50</PackageTargetFallback>
    <Authors>Zhicheng Su</Authors>
    <Company />
    <PackageLicenseUrl>http://www.opensource.org/licenses/mit-license.php</PackageLicenseUrl>
    <Copyright>Copyright 2018 Zhicheng Su</Copyright>
    <PackageProjectUrl>https://github.com/zsu/GenericRepository</PackageProjectUrl>
    <RepositoryUrl>https://github.com/zsu/GenericRepository</RepositoryUrl>
    <Description>Generic repository</Description>
    <PackageTags>Repository, Entity Framework Core</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.7</Version>
    <AssemblyVersion>1.0.7.0</AssemblyVersion>
    <RootNamespace>GenericRepository</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GenericRepository.EntityFrameworkCore" Version="1.2.1" />
    <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" Version="2.1.1" />
  </ItemGroup>

</Project>
